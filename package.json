{
  "name": "@mgeduld/react-videojs",
  "version": "1.0.11",
  "description": "A react wrapper for the video.js framwork.",
  "repository": "GrovoLearning/react-videojs",
  "main": "./lib/index.js",
  "scripts": {
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/react-videojs.js --config webpack.config.development.js",
    "build:umd:min": "webpack src/index.js dist/react-videojs.min.js --config webpack.config.production.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "watch": "webpack-dev-server --hot --inline",
    "test": "babel-tape-runner 'src/**/*.spec.js'",
    "prepublish": "npm run build"
  },
  "author": "Marcus Geduld",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-runtime": "^5.8.20",
    "lodash": "^3.10.1",
    "videojs": "^4.12.15"
  },
  "peerDependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "5.8.24",
    "babel-eslint": "4.1.3",
    "babel-loader": "5.3.2",
    "babel-tape-runner": "^1.2.0",
    "cheerio": "^0.19.0",
    "clean-webpack-plugin": "^0.1.3",
    "eslint-loader": "^1.0.0",
    "gulp": "^3.9.0",
    "gulp-run": "^1.6.11",
    "gulp-watch": "^4.3.5",
    "jsdom": "^6.5.1",
    "tape": "^4.2.1",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.10.1"
  }
}
